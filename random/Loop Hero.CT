function lua_aobscan(bytes)
	local moduleStrSize = getModuleSize(process)
	if moduleStrSize ~= nil then
		local memScanner = createMemScan()
		local memFoundList = createFoundList(memScanner)
		memScanner.firstScan(
		soExactValue,vtByteArray,rtRounded,bytes,nil,
		getAddress(process),(getAddress(process)+moduleStrSize),"",
		fsmNotAligned,"",true,false,false,false)
		memScanner.waitTillDone()
		memFoundList.initialize()
		local foundAdder = nil
		if memFoundList.Count == 1 then
			foundAdder = memFoundList.Address[0]
		elseif memFoundList.Count &gt; 1 then
			print("Array of Byte not unique: "..bytes)
		else
            print("Array of Byte not found: "..bytes)
        end
		memScanner.destroy()
		memFoundList.destroy()
		return foundAdder
	else
		print("Module "..process.." not found")
		return nil
	end
	return nil
end
function initializeSymbols()
	local luaEngine = GetLuaEngine()
    luaEngine.MenuItem5.doClick()
	for i = 1,#aobList do createThread(createSymbol(aobList[i])); if getAddressSafe(aobList[i][2]) then print("Successfully hooked: "..aobList[i][2]) end end
    luaEngine.hide()
end
function createSymbol(aobItem)
	local opAddr = lua_aobscan(aobItem[1])
    local addr = getAddressFromOpcode(opAddr)
    if addr then
	    unregisterSymbol(aobItem[2])
        registerSymbol(aobItem[2],addr)
    else
		unregisterSymbol(aobItem[2])
        registerSymbol(aobItem[2],opAddr)
    end
end
function destroySymbols()
	for i = 1,#aobList do unregisterSymbol(aobList[i][2]) end
end
function getAddressFromOpcode(opAddr)
	local disassembler = createDisassembler()
	disassembler.disassemble(opAddr)
    local disTable = disassembler.getLastDisassembleData()
    if disTable.modrmValueType == 1 then return disTable.modrmValue end
	return nil
end
function getAddressFromCall(opAddr)
	local disassembler = createDisassembler()
	disassembler.disassemble(opAddr)
    local disTable = disassembler.getLastDisassembleData()
    if disTable.isCall or disTable.isJump or disTable.isConditionalJump then return disTable.parameterValue end
	return nil
end
function checkForDuplicate(name)
	local addList = getAddressList()
	if addList.getMemoryRecordByDescription(name) then return true else return false end
end
function createRecord(base,desc,topRec)
	local addList = getAddressList()
	local newRec = addList.createMemoryRecord()
	newRec.Description = desc
	newRec.Address = base
	newRec.Type = 5
	newRec.AppendToEntry(topRec)
end
function populatePointers(mainRec)
	local baseAddr = getAddressSafe('[[[[baseGame]+520]]]')
	for i = 0,999 do
    	local tempAddr = readPointer(baseAddr+i*8)
    	if tempAddr and tempAddr ~= 0 then
		  local currAddr = readPointer(tempAddr+0x18)
          if currAddr and currAddr ~= 0 then
		    local checkVal = readDouble(currAddr)
		    local statBase = currAddr+0x10
		    local name = getStatType(checkVal)
		    if not checkForDuplicate(name) and name then createRecord(statBase,name,mainRec) end
          end
        end
	end
end
function removeRecords(mainRec)
	if mainRec ~= nil then
	    while mainRec.Count &gt; 0 do
	       mainRec.Child[0]:Delete()
	    end
    end
end
if syntaxcheck then return end
[ENABLE]
initializeSymbols()
[DISABLE]
destroySymbols()
